    "mongodb": "^3.0.4",
    "mongoose": "^5.0.9",
    "morgan": "^1.9.0",
    "mustache": "^2.3.0",
    "nodemailer": "^4.6.2",
    "nodemailer-direct-transport": "^3.3.2",
    "npm": "^5.7.1",
    "oauth2-server": "^3.0.0",
    "papaparse": "^4.3.7",
    "react": "^16.2.0",
    "react-debounce-input": "^3.2.0",
    "react-scripts": "^0.9.5",
    "simple-get": "^2.7.0",
    "xhr2": "^0.1.4"
